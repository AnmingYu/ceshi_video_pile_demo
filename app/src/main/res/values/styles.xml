    </style>

    <style name="tv_browsing">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:gravity">center</item>


    </style>

    <style name="shoujimain">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/colorBlueness</item>
    </style>

    <style name="mainContentshouji">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/colorGray</item>
    </style>
    <!--文字信息-->
    <style name="detailsForms">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorshallowBlue</item>
        <item name="android:textSize">20dp</item>
    </style>

    <style name="detailsFormsContent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
